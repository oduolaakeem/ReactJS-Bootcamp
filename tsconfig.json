{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react",
    "out": "./build/ts/tsc.js",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": true
  },
  "filesGlob": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "app/**/*.json",
    "typings/*.ts",
    "typings/**/*.ts",
    "!node_modules/**"
  ],
  "files": [
    "app/actions/AppActions.d.ts",
    "app/actions/AppActions.ts",
    "app/components/App/App.d.ts",
    "app/components/Header/Header.d.ts",
    "app/components/MovieList/MovieList.d.ts",
    "app/components/MovieTile/MovieTile.d.ts",
    "app/components/Rating/Rating.d.ts",
    "app/constants/AppConstants.ts",
    "app/dispatcher/AppDispatcher.ts",
    "app/mock/movies.ts",
    "app/models/Movie.d.ts",
    "app/models/Movies.ts",
    "app/stores/MovieStore.ts",
    "app/util/api.ts",
    "app/app.tsx",
    "app/components/App/App.tsx",
    "app/components/Header/Header.tsx",
    "app/components/MovieList/MovieList.tsx",
    "app/components/MovieTile/MovieTile.tsx",
    "app/components/Rating/Rating.tsx",
    "typings/tsd.d.ts",
    "typings/flux/flux.d.ts",
    "typings/lodash/lodash.d.ts",
    "typings/node/node.d.ts",
    "typings/react/react-global.d.ts",
    "typings/react/react.d.ts"
  ],
  "exclude": []
}
