{
  "name": "ReactJS-Bootcamp",
  "version": "0.0.1",
  "description": "",
  "repository": "",
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "0.0.0.0",
    "devPort": 8001
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "start-windows": "set NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "dependencies": {
    "classnames": "^2.1.1",
    "flux": "^2.0.3",
    "font-awesome-webpack": "0.0.4",
    "normalize.css": "^3.0.3",
    "page": "^1.6.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "url-loader": "^0.5.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^5.8.3",
    "babel-loader": "^5.0.0",
    "babel-plugin-react-transform": "^1.0.5",
    "babel-plugin-rewire": "^0.1.23-beta-3",
    "babel-runtime": "^5.3.3",
    "chai": "^3.3.0",
    "css-loader": "^0.20.2",
    "eslint": "^1.7.2",
    "eslint-plugin-react": "^3.6.3",
    "extract-text-webpack-plugin": "^0.8.0",
    "file-loader": "^0.8.1",
    "glob": "^5.0.6",
    "html-loader": "^0.3.0",
    "isparta-loader": "^1.0.0",
    "json-loader": "^0.5.1",
    "karma": "^0.13.3",
    "karma-chrome-launcher": "^0.2.1",
    "karma-cli": "0.1.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-source-map-support": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.1",
    "lodash": "^3.8.0",
    "mocha": "^2.2.4",
    "mocha-loader": "^0.7.1",
    "node-libs-browser": "^0.5.0",
    "opn": "^3.0.2",
    "postcss-loader": "^0.7.0",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-hmr": "^1.0.1",
    "rimraf": "^2.3.3",
    "sass-loader": "^3.0.0",
    "sinon": "^1.14.1",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "webpack": "^1.9.5",
    "webpack-dev-server": "^1.8.2"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
